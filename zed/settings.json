{
  "assistant": {
    "default_model": {
      "model": "gemini-2.0-flash-exp",
      "provider": "google"
    },
    "version": "2"
  },
  "auto_signature_help": true,
  "base_keymap": "VSCode",
  "buffer_font_family": "Iosevka",
  "buffer_font_size": 14,
  "buffer_font_weight": 601,
  "buffer_line_height": "standard",
  "current_line_highlight": "none",
  "cursor_blink": false,
  "features": {
    "inline_completion_provider": "supermaven"
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "inlay_hints": {
    "enabled": true,
    "show_background": true,
    "show_other_hints": true,
    "show_type_hints": true
  },
  "languages": {
    "C": {
      "show_inline_completions": true
    },
    "C++": {
      "show_inline_completions": true,
      "tab_size": 2
    },
    "CSS": {
      "tab_size": 2
    },
    "HTML": {
      "show_inline_completions": true,
      "tab_size": 2
    },
    "JSONC": {
      "show_inline_completions": true
    },
    "LaTeX": {
      "show_inline_completions": true,
      "tab_size": 2
    },
    "Lua": {
      "show_inline_completions": true
    }
  },
  "relative_line_numbers": true,
  "show_inline_completions": true,
  "soft_wrap": "editor_width",
  "terminal": {
    "blinking": "off",
    "font_size": 14,
    "font_weight": 601
  },
  "theme": {
    "dark": "Pink Light",
    "light": "Pink Light",
    "mode": "system"
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
    "selections_menu": false
  },
  "ui_font_size": 16,
  "ui_font_weight": 501,
  "vim_mode": true
}
